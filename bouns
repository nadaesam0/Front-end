#include <iostream>
using namespace std;

int revers(int num) {
    int reversed = 0;
    while (num != 0) {
        int remainder = num % 10;
        reversed = reversed * 10 + remainder; 
        num /= 10; 
    }
    return reversed;
}

int main() {
    int number;
    cin >> number;
     cout << revers(number);
    return 0;
} 
